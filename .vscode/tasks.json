{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "start",
			"isBackground": true,
			"options": {"env": {
				"NODE_OPTIONS":"--openssl-legacy-provider"
			}},
			"problemMatcher": [{
				"pattern": [
					{
						"regexp": ".",
						"file": 1,
						"location": 2,
						"message": 3
					}
				],
				"background": {
					"activeOnStart": true,
					"beginsPattern": ".",
					"endsPattern": "Compiled successfully."
				}
			}],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "npm start"
		},
		{
			"type": "npm",
			"script": "test",
			"problemMatcher": [],
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}